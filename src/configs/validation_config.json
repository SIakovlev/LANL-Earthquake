{
  "train_data": "data/train_good_features.h5",
  "summary_dest": "data/summary_temp.csv",
  "preproc": {
    "name": "StandardScaler"
  },
 "folds":
          [
            {
              "name":"KFold",
              "shuffle": true,
              "n_splits": 5
            },
            {
              "name":"CustomFold",
              "n_splits": 10,
              "pad": 15,
              "fragmentation": 0.1,
              "shuffle": true
            }
          ],

  "metrics": ["mean_absolute_error", "mean_squared_error"],



  "validate":
          [
            {
              "model": {
                "XGBRegressor": {
                  "eta": 0.003,
                  "max_depth": 9,
                  "n_estimators": 1000,
                  "subsample": 0.9,
                  "objective": "gpu:reg:linear",
                  "eval_metric": "mae",
                  "silent": true,
                  "tree_method": "gpu_hist",
                  "nthread": -1
                }
              }
            }

          ]

}