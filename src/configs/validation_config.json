{
  "train_data": "data/train_good_smoothed.h5",
  "summary_dest": "data/summary_train_.csv",
  "predict_directory": "data/predict",
  "models_directory": "data/saved_models",
  "preproc": {
    "name": "StandardScaler"
  },
 "folds":
          [
            {
              "name":"KFold",
              "shuffle": true,
              "n_splits": 5
            }

          ],

  "metrics": ["mean_absolute_error", "mean_squared_error"],



  "validate":
          [
            {
              "model": { "Ridge": {},
                          "RandomForestRegressor": {"criterion":"mse",
                                                   "max_depth": 9,
                                                   "n_estimators":1000
                                                  }
                       }

            },
            {
              "model": {
                "XGBRegressor": {
                  "eta": 0.003,
                  "max_depth": 9,
                  "n_estimators": 1000,
                  "subsample": 0.9,
                  "objective": "reg:linear",
                  "eval_metric": "mae",
                  "silent": true,
                  "nthread": -1
                }
              }
            }

          ]

}