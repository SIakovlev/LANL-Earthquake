{
  "train_data": "../../data/e5.h5",
  "summary_dest": "../../data/submit_12_05_2/summary.csv",
  "predict_directory": "../../data/submit_12_05_2/predict",
  "models_directory": "../../data/submit_12_05_2/saved_models",
  "preproc": {
    "name": "StandardScaler"
  },
 "folds":
          [
            {
              "name":"CustomFold",
              "n_splits": 9,
              "pad": 150,
              "fragmentation": 0,
              "shuffle": true
            }
          ],

  "metrics": ["mean_absolute_error"],

  "validate":
          [
            {
              "model": {
                "XGBRegressor": {
                  "eta": 0.01,
                  "max_depth": 4,
                  "n_estimators": 51,
                  "subsample": 0.7,
                  "objective": "gpu:reg:linear",
                  "eval_metric": "mae",
                  "tree_method": "gpu_hist",
                  "verbosity": 3,
                  "nthread": 15
                }
              }
            }
          ]

}