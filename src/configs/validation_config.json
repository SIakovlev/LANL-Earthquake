{
  "train_data": "data/train_wavelet_proceded_test.h5",
  "summary_dest": "data/summary_wavelet_int_150000_1000_temp.csv",
  "models_directory": "data/models_wav_temps/",
  "preproc": {
    "name": "StandardScaler"
  },
 "folds":
          [
            {
              "name":"CustomFold",
              "n_splits": 1,
              "pad": 150,
              "fragmentation": 0.2,
              "shuffle": true
            }
          ],

  "metrics": ["mean_absolute_error", "mean_squared_error"],



  "validate":
          [
            {
              "model": {
                "Ridge": {
                }
              }
            },
            {
              "model": {
                "XGBRegressor": {
                  "eta": 0.01,
                  "max_depth": 8,
                  "seed" :17,
                  "n_estimators": 600,
                  "subsample": 0.75,
                  "objective": "gpu:reg:linear",
                  "eval_metric": "mae",
                  "silent": true,
                  "tree_method": "gpu_hist"
                }
              }
            }

          ]

}