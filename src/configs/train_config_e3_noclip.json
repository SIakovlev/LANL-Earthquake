{
  "train_data_fname": "../../data/e3_noclip.h5",
  "summary_dest_fname": "../../data/e3_noclip_summary.pickle",
  "img_folder_path": "../../data/e3",
  "verbose": false,

  "folds":{
      "name":"CustomFold",
      "n_splits": 9,
      "pad": 150,
      "fragmentation": 0,
      "shuffle": true
            },

  "metrics": ["mean_absolute_error", "mean_squared_error"],

  "preproc": {
    "name": "StandardScaler"
  },

  "model": {
    "name": "XGBRegressor",
    "eta": 0.01,
    "max_depth": 4,
    "n_estimators": 51,
    "subsample": 0.7,
    "objective": "gpu:reg:linear",
    "eval_metric": "mae",
    "tree_method": "gpu_hist",
    "verbosity": 3,
    "nthread": 15
  }
}