{
  "train_data_fname": "../../data/train_processed_e1.h5",
  "summary_dest_fname": "../../data/summary/summary.pickle",


  "folds":{
      "name":"CustomFold",
      "n_splits": 10,
      "pad": 150,
      "fragmentation": 0.001,
      "shuffle": true
            },

  "metrics": ["mean_absolute_error", "mean_squared_error"],

  "model": {
    "name": "XGBRegressor",
    "eta": 0.01,
    "max_depth": 9,
    "n_estimators": 2000,
    "subsample": 0.9,
    "objective": "gpu:reg:linear",
    "eval_metric": "mae",
    "tree_method": "gpu_hist",
    "verbosity": 3,
    "nthread": 15
  }
}