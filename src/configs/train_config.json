{
  "train_data_fname": "../../data/original/train_processed.h5",
  "summary_dest_fname": "../../data/summary/summary.pickle",


  "folds":{
      "name":"CustomFold",
      "n_splits": 10,
      "pad": 15,
      "fragmentation": 0.1,
      "shuffle": true
            },

  "metrics": ["mean_absolute_error", "mean_squared_error"],

  "model": {
    "name": "XGBRegressor",
    "eta": 0.003,
    "max_depth": 9,
    "n_estimators":1000,
    "subsample": 0.9,
    "objective": "reg:linear",
    "eval_metric": "mae",
    "verbosity": 3,
    "n_jobs": 15

  }
}