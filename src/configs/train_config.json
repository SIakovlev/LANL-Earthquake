{
  "train_data_fname": "../../data.nosync/train_good_smoothed_better_e1.h5",
  "summary_dest_fname": "../../data/summary/summary.pickle",


  "folds":{
      "name":"CustomFold",
      "n_splits": 10,
      "pad": 150,
      "fragmentation": 0,
      "shuffle": true
            },

  "metrics": ["mean_absolute_error", "mean_squared_error"],

  "preproc": {
    "name": "StandardScaler"
  },

  "model": {
    "name": "XGBRegressor",
    "eta": 0.01,
    "max_depth": 6,
    "n_estimators": 300,
    "subsample": 0.9,
    "objective": "reg:linear",
    "eval_metric": "mae",
    "verbosity": 3,
    "nthread": 15
  }
}