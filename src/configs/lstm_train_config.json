{
  "train_data_fname": "../../data/original/train_processed.h5",
  "summary_dest_fname": "../../data/summary/summary.pickle",

  "folds":{
      "name":"CustomFold",
      "n_splits": 10,
      "pad": 15,
      "fragmentation": 0.01,
      "shuffle": true
            },

  "metrics": ["mean_absolute_error", "mean_squared_error"],

  "model": {
    "name": "LstmNet",
    "in_features": [11, 392],
    "out_features": 1,
    "hidden_layers": [512, 128],
    "lstm_hidden_size" : 256,
    "dropout": 0.3,
    "device": "cuda",
    "learning_rate": 0.001,
    "minibatch_size": 128,
    "num_epochs": 40
  }
}